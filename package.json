{
  "name": "a11y",
  "productName": "Accessibility Tools",
  "description": "Accessibility Dev Tools",
  "version": "1.0.3",
  "author": {
    "name": "Clym Inc",
    "url": "https://clym.io/",
    "email": "support@clym.io"
  },
  "homepage": "https://clym.io/",
  "private": true,
  "main": "build/main/index.js",
  "license": "MIT",
  "scripts": {
    "next:dev": "next dev",
    "next:build": "next build",
    "next:start": "next start",
    "next:lint": "next lint",
    "postinstall": "electron-builder install-app-deps",
    "build": "run-s next:build electron:build",
    "predist": "rimraf dist && npm run electron:rebuild",
    "dist": "run-s next:build electron:dist",
    "dist:win": "run-s predist build electron:dist:win postdist",
    "dist:win:release": "cross-env NODE_ENV='production' run-s predist build electron:dist:win postdist",
    "dist:mac": "run-s predist build electron:dist:mac postdist",
    "dist:mac:release": "cross-env NODE_ENV='production' run-s predist build electron:dist:mac postdist",
    "postdist": "run-s electron:dist:clean electron:rebuild",
    "dev": "npm-run-all --parallel electron:dev next:dev",
    "electron:dist": "run-s electron:dist:win electron:dist:mac",
    "electron:dist:clean": "rimraf --glob dist/*-unpacked dist/mac-arm64 dist/mac dist/mac-universal dist/builder-*",
    "electron:dist:mac": "run-s electron:rebuild electron:dist:mac:run",
    "electron:dist:mac:run": "electron-builder --mac --universal --publish never --config electron-builder.config.js",
    "electron:dist:linux": "run-s electron:dist:linux:run",
    "electron:dist:linux:run": "electron-builder --linux --x64 --publish never --config electron-builder.config.js",
    "electron:dist:win": "run-s electron:dist:win:run",
    "electron:dist:win:run": "electron-builder --win --x64 --publish never --config electron-builder.config.js",
    "electron:build": "node esbuild.config.mjs",
    "electron:build_watch": "node esbuild.config.mjs --watch",
    "electron:dev": "npm-run-all --parallel electron:build_watch electron:watch",
    "electron:watch": "cross-env NODE_ENV='development' npx electronmon . --trace-deprecation",
    "lint": "eslint -c ./.eslintrc.js ./src --fix",
    "electron:rebuild": "npx electron-rebuild"
  },
  "dependencies": {
    "@electron-toolkit/utils": "^3.0.0",
    "@emotion/cache": "^11.13.1",
    "@emotion/react": "^11.13.3",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.13.0",
    "@mui/material": "^6.1.4",
    "@mui/material-nextjs": "^6.1.4",
    "@mui/x-charts": "^8.0.0-beta.3",
    "ace-builds": "^1.38.0",
    "axios": "^1.7.7",
    "cheerio": "^1.0.0",
    "classnames": "^2.5.1",
    "date-fns": "^4.1.0",
    "electron-log": "^5.4.3",
    "electron-updater": "^6.6.2",
    "fs-extra": "^11.3.0",
    "get-port-please": "^3.1.2",
    "joi": "^17.13.3",
    "next": "^14.2.31",
    "node-abi": "^4.12.0",
    "node-xlsx": "^0.24.0",
    "react": "^18",
    "react-ace": "^14.0.1",
    "react-datepicker": "^8.3.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18",
    "react-resizable": "^3.0.5",
    "sass": "^1.89.2",
    "sequelize": "^6.37.5",
    "sqlite3": "^5.1.7",
    "xml2js": "^0.6.2",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@electron/notarize": "^3.0.1",
    "@stylistic/eslint-plugin": "^1.6.1",
    "@stylistic/eslint-plugin-js": "^1.6.1",
    "@stylistic/eslint-plugin-jsx": "^1.6.1",
    "@svgr/webpack": "^8.1.0",
    "axe-core": "^4.10.2",
    "cross-env": "^7.0.3",
    "electron": "^37.2.5",
    "electron-builder": "^26.0.12",
    "electronmon": "^2.0.3",
    "esbuild": "^0.25.8",
    "eslint": "^8.57.1",
    "eslint-config-next": "^15.4.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3",
    "prettier-eslint": "^16.4.2",
    "rimraf": "^6.0.1",
    "tsup": "^8.3.0"
  },
  "electronmon": {
    "patterns": [
      "!src/**/*",
      "!*.sqlite",
      "!*.sqlite-journal",
      "!builds/**/*",
      "!bin/**/*",
      "!archives/**/*"
    ]
  }
}
